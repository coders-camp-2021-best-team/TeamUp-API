{
    "name": "teamup-api",
    "version": "1.0.0",
    "license": "GPL-3.0",
    "scripts": {
        "prepare": "husky install",
        "prebuild": "rimraf build/",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "start": "node build/src/index.js",
        "start:dev": "nodemon -r dotenv/config --inspect=0.0.0.0:9229 build/src/index.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "format": "prettier --ignore-unknown --ignore-path .gitignore --write .",
        "lint": "eslint --ext .js,.ts src/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/coders-camp-2021-best-team/TeamUp-API.git"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "connect-redis": "^6.1.1",
        "express": "^4.17.3",
        "express-session": "^1.17.2",
        "joi": "^17.6.0",
        "mysql": "^2.18.1",
        "redis": "^4.0.4",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "typeorm": "^0.2.43",
        "typeorm-logger-adaptor": "^1.0.4",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/connect-redis": "^0.0.18",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.18",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "@typescript-eslint/parser": "^5.12.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.9.0",
        "eslint-config-prettier": "^8.3.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "lint-staged": ">=12",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "engines": {
        "yarn": ">= 1.19.1"
    }
}
